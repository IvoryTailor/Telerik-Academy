using System;

class Program
{
    static void Main()
    {
        int number = int.Parse(Console.ReadLine());
        for (int i = 1; i <= number; i++)
        {
            if (i == number)
            {
                Console.WriteLine(i);
            }
            else
            {
                Console.Write("{0} ", i);
            }
        }
    }
}
