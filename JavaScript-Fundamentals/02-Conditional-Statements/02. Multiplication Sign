function solve(args) {
    var num1 = +args[0];
    var num2 = +args[1];
    var num3 = +args[2];
    if (num1 == 0 || num2 == 0 || num3 == 0) {
        console.log(0);
    }else if ((num1>0&&(num2<0&&num3<0))||(num2>0&&(num1<0&&num3<0))||(num3>0&&(num1<0&&num2<0))||(num1>0&&num2>0&&num3>0)) {
        console.log("+")
    }
    else {
        console.log("-")
    }
}
